		OP1 = level;
		TMP = reg_r16(0x18);
		TMP -= 2;
		reg_w16(0x18, TMP);
		any_w16(TMP, PC);
		PC = any_r16(0x2000+2*OP1);
		standard_irq_callback(OP1);
		check_irq();
