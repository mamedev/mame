macro post_indirect $size $cycles1 $cycles2
	if(OPI & 0x01) {
		reg_w16(OPI, OP1 + $size);
		next($cycles2); // +4 when external
	} else {
		next($cycles1); // +4 when external
	}

eadr  indexed_1
	OPI = read_pc();
	OP1 = read_pc();
	if(OPI & 0x01) {
		OPI &= 0xfe;
		OP1 |= read_pc() << 8;
	} else
		OP1 = INT8(OP1);
	if(OPI) {
		OP1 += reg_r16(OPI);
	}

eadr  indexed_2
	OPI = read_pc();
	OP1 = read_pc();
	if(OPI & 0x01) {
		OPI &= 0xfe;
