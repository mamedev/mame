macro   ldi
	@rm HL
	@wm DE
	2 * @nomreq_addr DE
	HL++;
	DE++;
	BC--;
	{
		QT = 0;
		// keep SZC
		m_f.yx_val = ((A + TDAT8) << 4) | ((A + TDAT8) & 0x0f);
		m_f.h_val = m_f.n = 0;
		m_f.pv_val = !BC;
	}

macro   r800:ldi
