ffff
	if (m_icount <= 0) {
		m_ref = 0xffff00;
		return;
	}
	if (m_service_attention) {
		if (m_busrq_state) { // SA_BUSRQ
			if (!m_busack_state) {
				m_busack_state = 1;
				set_service_attention<SA_BUSACK, 1>();
				m_busack_cb(1);
			}
			if (m_icount > 0)
				m_icount = 0;
			m_ref = 0xffff00;
			return;
		} else if (m_busack_state) { // SA_BUSACK
			m_busack_state = 0;
			set_service_attention<SA_BUSACK, 0>();
			m_busack_cb(0);
		}
		@check_interrupts
		set_service_attention<SA_AFTER_EI, 0>();
		if (HAS_LDAIR_QUIRK)
			set_service_attention<SA_AFTER_LDAIR, 0>();
		if (m_halt) { // SA_HALT
			debugger_wait_hook();
			@rop
			PC--;
			continue;
		}
	}
	PRVPC = PC;
	debugger_instruction_hook(PC);
	@rop
	m_ref = (0x00 << 16) | (TDAT8 << 8);
	goto process;


