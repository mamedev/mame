macro   ex_sp   %dd
	@pop
	@nomreq_addr SP-1
	@wm16_sp %dd
	%dd = TDAT;
	2 * @nomreq_addr SP
	WZ = TDAT;

macro   add16   %dd1 %dd2
{
	@nomreq_ir 7
	const u32 res = %dd1 + %dd2;
	WZ = %dd1 + 1;
	{
		QT = 0;
		// keep szv
		m_f.yx_val = res >> 8;
		m_f.h_val = (%dd1 ^ res ^ %dd2) >> 8;
		m_f.n = 0;
		m_f.c = res & 0x10000;
	}
	%dd1 = (u16)res;
}

