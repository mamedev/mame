macro   rrd
	@rm HL
	WZ = HL+1;
	4 * @nomreq_addr HL
	TDAT_H = TDAT8;
	TDAT8 = (TDAT8 >> 4) | (A << 4);
	@wm HL
	A = (A & 0xf0) | (TDAT_H & 0x0f);
	{
		QT = 0;
		// keep C
		m_f.s_val = m_f.z_val = m_f.pv_val = A;
		m_f.yx_val = A;
		m_f.h_val = m_f.n = 0;
	}

macro   rld
