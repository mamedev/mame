		m_f.h_val = m_f.n = 0;
		m_f.pv_val = !BC;
	}

macro   cpi
{
	@rm HL
	5 * @nomreq_addr HL
	WZ++;
	HL++;
	BC--;
	u8 res = A - TDAT8;
	{
		QT = 0;
		// keep C
		m_f.s_val = m_f.z_val = res;
		m_f.h_val = A ^ TDAT8 ^ res;
		if (m_f.h())
			res -= 1;
		m_f.yx_val = (res << 4) | (res & 0x0f);
		m_f.pv_val = !BC;
		m_f.n = 1;
	}
}

