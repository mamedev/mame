macro   ini
{
	@nomreq_ir 1
	@in BC
	WZ = BC + 1;
	B--;
	@wm HL
	HL++;
	const u16 t = ((C + 1) & 0xff) + TDAT8;
	{
		QT = 0;
		m_f.s_val = m_f.z_val = B;
		m_f.yx_val = B;
		m_f.h_val = t >> 4;
		m_f.pv_val = (t & 0x07) ^ B;
		m_f.n = TDAT8 & SF;
		m_f.c = t & 0x100;
	}
}

