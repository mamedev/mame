	}
	%dd1 = (u16)res;
}

macro   adc_hl   %dd
{
	@nomreq_ir 7
	const u32 res = HL + %dd + m_f.c;
	WZ = HL + 1;
	{
		QT = 0;
		m_f.s_val = m_f.z_val = ((res & 0x8000) >> 8) | (u16(res) != 0);
		m_f.yx_val = res >> 8;
		m_f.h_val = (HL ^ res ^ %dd) >> 8;
		m_f.pv_val = !((%dd ^ HL ^ 0x8000) & (%dd ^ res) & 0x8000);
		m_f.n = 0;
		m_f.c = res & 0x10000;
	}
	HL = (u16)res;
}

