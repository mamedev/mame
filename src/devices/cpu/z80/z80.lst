macro   ldir
	call ldi
	if (BC != 0) {
		TADR = DE;
		5 * call nomreq_addr
		PC -= 2; WZ = PC + 1; F &= ~(YF | XF); F |= (PC >> 8) & (YF | XF);
	}

macro   r800:ldir
	call ldi
	if (BC != 0) {
		TADR = DE;
		call nomreq_addr
		PC -= 2; WZ = PC + 1; F &= ~(YF | XF); F |= (PC >> 8) & (YF | XF);
	}

