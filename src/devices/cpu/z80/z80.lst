macro   ld_a_r
	@nomreq_ir 1
	A = (m_r & 0x7f) | m_r2;
	{
		QT = 0;
		// keep C
		m_f.s_val = m_f.z_val = A;
		m_f.yx_val = A;
		m_f.h_val = m_f.n = 0;
		m_f.pv_val = !m_iff2;
	}
	if (HAS_LDAIR_QUIRK)
		set_service_attention<SA_AFTER_LDAIR, 1>();

