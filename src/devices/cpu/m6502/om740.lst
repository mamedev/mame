bbc_bzr
	TMP = read_pc();
	TMP2 = read(TMP);
	read_pc_noinc();
	if(!(TMP2 & (1 << ((IR>>5) & 7)))) {
		TMP = read_pc_noinc();
		read_dummy(set_l(PC, PC+1));
		PC++;
		read_dummy(set_l(PC, PC+int8_t(TMP)));
		PC += int8_t(TMP);
	} else {
		read_pc();
	}
	prefetch();

