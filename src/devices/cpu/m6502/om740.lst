# m740 opcodes
set_imp
	read_pc_noinc();
	P |= F_T;
	inst_state_base = 0x100;
	prefetch();

clt_imp
	read_pc_noinc();
	P &= ~F_T;
	inst_state_base = 0;
	prefetch();

ldm_imz
	TMP = read_pc();
	TMP2 = read_pc();
	write(TMP2, TMP);
	prefetch();

dec_acc
	read_pc_noinc();
	A--;
	set_nz(A);
	prefetch();

inc_acc
	read_pc_noinc();
	A++;
	set_nz(A);
	prefetch();

jsr_spg
	TMP = read_pc_noinc();
	read_dummy(SP);
	write(SP, PC>>8);
	dec_SP();
	write(SP, PC);
	dec_SP();
	TMP = set_h(TMP, 0xff);
	PC = TMP;
	prefetch();

clb_bac
