<mamelayout version="2">

<!-- define elements -->

	<element name="black">
		<rect><color red="0.0" green="0.0" blue="0.0" /></rect>
	</element>
	<element name="darkgray">
		<rect><color red="0.25" green="0.25" blue="0.25" /></rect>
	</element>
	<element name="lightgray">
		<rect><color red="0.6" green="0.6" blue="0.6" /></rect>
	</element>
	<element name="yellow">
		<rect><color red="0.8" green="0.8" blue="0.1" /></rect>
	</element>
	<element name="blue">
		<rect><color red="0.2" green="0.5" blue="0.8" /></rect>
	</element>


<!-- throttle -->

	<element name="text_throttle">
		<text string="THROTTLE:" align="1"><color red="0.8" green="0.8" blue="0.8" /></text>
	</element>
	<element name="text_throttle_j">
		<text string="マスコン:" align="1"><color red="0.8" green="0.8" blue="0.8" /></text>
	</element>

	<element name="text_dot">
		<text string="." align="0"><color red="1.0" green="1.0" blue="0.0" /></text>
	</element>
	<element name="text_speed">
		<text string="km/h" align="1"><color red="1.0" green="1.0" blue="0.0" /></text>
	</element>

	<element name="counter_throttle" defstate="0">
		<simplecounter maxstate="9" digits="1" align="1">
			<color red="1.0" green="1.0" blue="1.0" />
		</simplecounter>
	</element>
	<element name="counter_speed1" defstate="0">
		<simplecounter maxstate="999" digits="3" align="2">
			<color red="1.0" green="1.0" blue="0.0" />
		</simplecounter>
	</element>
	<element name="counter_speed2" defstate="0">
		<simplecounter maxstate="9" digits="1" align="1">
			<color red="1.0" green="1.0" blue="0.0" />
		</simplecounter>
	</element>


<!-- brake -->

	<element name="text_brake">
		<text string="BRAKE:" align="1"><color red="0.8" green="0.8" blue="0.8" /></text>
	</element>
	<element name="text_brake_j">
		<text string="ブレーキ:" align="1"><color red="0.8" green="0.8" blue="0.8" /></text>
	</element>

	<element name="text_pressure">
		<text string="kg/cm²" align="1"><color red="1.0" green="1.0" blue="0.0" /></text>
	</element>

	<element name="counter_brake" defstate="0">
		<simplecounter maxstate="99" digits="1" align="1">
			<color red="1.0" green="1.0" blue="1.0" />
		</simplecounter>
	</element>
	<element name="counter_pressure1" defstate="0">
		<simplecounter maxstate="99" digits="2" align="2">
			<color red="1.0" green="1.0" blue="0.0" />
		</simplecounter>
	</element>
	<element name="counter_pressure2" defstate="0">
		<simplecounter maxstate="9" digits="1" align="1">
			<color red="1.0" green="1.0" blue="0.0" />
		</simplecounter>
	</element>


<!-- lamps -->

	<element name="lamp_white" defstate="0">
		<rect state="1"><color red="1.0" green="0.85" blue="0.65" /></rect>
		<rect state="0"><color red="0.2" green="0.2" blue="0.2" /></rect>
	</element>

	<element name="lamp_red" defstate="0">
		<rect state="1"><color red="1.0" green="0.1" blue="0.1" /></rect>
		<rect state="0"><color red="0.3" green="0.0" blue="0.0" /></rect>
	</element>

	<element name="text_pwr">
		<text string="PWR"><color red="0.0" green="0.0" blue="0.0" /></text>
	</element>
	<element name="text_ok">
		<text string="OK"><color red="0.0" green="0.0" blue="0.0" /></text>
	</element>
	<element name="text_eme">
		<text string="EME"><color red="0.0" green="0.0" blue="0.0" /></text>
	</element>
	<element name="text_door">
		<text string="doors"><color red="0.9" green="0.1" blue="0.1" /></text>
	</element>

	<element name="text_pwr_j">
		<text string="三相"><color red="0.0" green="0.0" blue="0.0" /></text>
	</element>
	<element name="text_ok_j">
		<text string="常用"><color red="0.0" green="0.0" blue="0.0" /></text>
	</element>
	<element name="text_eme_j">
		<text string="非常"><color red="0.0" green="0.0" blue="0.0" /></text>
	</element>
	<element name="text_door_j">
		<text string="車内信号"><color red="0.9" green="0.1" blue="0.1" /></text>
	</element>

	<element name="text_1">
		<text string="1"><color red="0.0" green="0.0" blue="0.0" /></text>
	</element>
	<element name="text_2">
		<text string="2"><color red="0.0" green="0.0" blue="0.0" /></text>
	</element>
	<element name="text_3">
		<text string="3"><color red="0.0" green="0.0" blue="0.0" /></text>
	</element>

	<element name="lamp_door" defstate="0">
		<disk state="1"><color red="1.0" green="0.85" blue="0.65" /></disk>
		<disk state="0"><color red="0.2" green="0.2" blue="0.2" /></disk>
	</element>

	<element name="lamp_start" defstate="0">
		<disk state="1"><color red="1.0" green="1.0" blue="0.3" /></disk>
		<disk state="0"><color red="0.2" green="0.2" blue="0.0" /></disk>
	</element>

	<element name="lamp_redd" defstate="0">
		<disk>
			<color red="0.0" green="0.0" blue="0.0" />
		</disk>
		<disk state="1">
			<color red="1.0" green="0.1" blue="0.1" />
			<bounds x="0.1" y="0.1" width="0.79" height="0.79" />
		</disk>
		<disk state="0">
			<color red="0.4" green="0.04" blue="0.04" />
			<bounds x="0.1" y="0.1" width="0.79" height="0.79" />
		</disk>
	</element>


<!-- build screen -->

	<group name="shared">
		<bounds left="0" right="400" top="0" bottom="330" />
		<screen index="0">
			<bounds left="0" right="400" top="0" bottom="300" />
		</screen>

		<!-- static background -->
		<element ref="black">
			<bounds left="0" right="400" top="300" bottom="330" />
		</element>
		<element ref="lightgray">
			<bounds left="0" right="113" top="302" bottom="330" />
		</element>
		<element ref="yellow">
			<bounds left="113" right="400" top="302" bottom="330" />
		</element>
		<element ref="darkgray">
			<bounds left="115" right="227" top="304" bottom="328" />
		</element>
		<element ref="lightgray">
			<bounds left="245" right="380" top="302" bottom="330" />
		</element>
		<element ref="black">
			<bounds left="244.75" right="245.5" top="302" bottom="330" />
		</element>
		<element ref="black">
			<bounds left="379.25" right="380.5" top="302" bottom="330" />
		</element>
		<element ref="blue">
			<bounds left="360" right="380" top="302" bottom="330" />
		</element>

		<!-- lamps -->
		<element name="lamp7" ref="lamp_white">
			<bounds x="8" y="304" width="17" height="24" />
		</element>
		<element name="lamp2" ref="lamp_white">
			<bounds x="31" y="304" width="17" height="24" />
		</element>
		<element name="lamp3" ref="lamp_red">
			<bounds x="54" y="304" width="17" height="24" />
		</element>

		<element name="lamp1" ref="lamp_door">
			<bounds x="86" y="314" width="14" height="14" />
		</element>
		<element name="lamp0" ref="lamp_start">
			<bounds x="231.5" y="317" width="9" height="9" />
		</element>

		<element name="lamp4" ref="lamp_white">
			<bounds x="258.5" y="304" width="17" height="24" />
		</element>
		<element name="lamp4" ref="lamp_white">
			<bounds x="281.5" y="304" width="17" height="24" />
		</element>
		<element name="lamp4" ref="lamp_white">
			<bounds x="304.5" y="304" width="17" height="24" />
		</element>
		<element ref="lamp_white">
			<bounds x="327.5" y="304" width="17" height="24" />
		</element>

		<element ref="text_1">
			<bounds xc="267" yc="316" width="14" height="10" />
		</element>
		<element ref="text_2">
			<bounds xc="290" yc="316" width="14" height="10" />
		</element>
		<element ref="text_3">
			<bounds xc="313" yc="316" width="14" height="10" />
		</element>

		<element name="lamp5" ref="lamp_redd">
			<bounds x="352" y="300" width="20" height="20" />
		</element>
		<element name="lamp6" ref="lamp_redd">
			<bounds x="370" y="300" width="20" height="20" />
		</element>

		<!-- meters -->
		<element name="counter2" ref="counter_speed1">
			<bounds x="120" y="316" width="13.5" height="10" />
		</element>
		<element ref="text_dot">
			<bounds x="133" y="316" width="3" height="10" />
		</element>
		<element name="counter3" ref="counter_speed2">
			<bounds x="136" y="316" width="4.25" height="10" />
		</element>
		<element ref="text_speed">
			<bounds x="144" y="316" width="30" height="10" />
		</element>

		<element name="counter4" ref="counter_pressure1">
			<bounds x="179" y="316" width="9" height="10" />
		</element>
		<element ref="text_dot">
			<bounds x="187.5" y="316" width="3" height="10" />
		</element>
		<element name="counter5" ref="counter_pressure2">
			<bounds x="190.5" y="316" width="4.25" height="10" />
		</element>

		<element ref="text_pressure">
			<bounds x="198.5" y="316" width="25" height="10" />
		</element>
	</group>

	<view name="Internal Layout (English)"> <!-- translated labels -->
		<bounds left="0" right="400" top="0" bottom="330" />
		<group ref="shared"><bounds left="0" right="400" top="0" bottom="330" /></group>

		<!-- lamp labels -->
		<element ref="text_pwr">
			<bounds xc="16.5" yc="316" width="14" height="8" />
		</element>

		<element ref="text_ok">
			<bounds xc="39.5" yc="316" width="14" height="8" />
		</element>

		<element ref="text_eme">
			<bounds xc="62.5" yc="316" width="14" height="8" />
		</element>

		<element ref="text_door">
			<bounds x="81" y="304" width="24" height="9" />
		</element>

		<!-- meters -->
		<element ref="text_throttle">
			<bounds x="120" y="306" width="40" height="10" />
		</element>
		<element name="counter0" ref="counter_throttle">
			<bounds x="163" y="306" width="10" height="10" />
		</element>

		<element ref="text_brake">
			<bounds x="179" y="306" width="27" height="10" />
		</element>
		<element name="counter1" ref="counter_brake">
			<bounds x="208" y="306" width="12" height="10" />
		</element>
	</view>

	<view name="Internal Layout (Japanese)"> <!-- original labels -->
		<bounds left="0" right="400" top="0" bottom="330" />
		<group ref="shared"><bounds left="0" right="400" top="0" bottom="330" /></group>

		<!-- lamp labels -->
		<element ref="text_pwr_j">
			<bounds xc="16.5" yc="316" width="14" height="8" />
		</element>

		<element ref="text_ok_j">
			<bounds xc="39.5" yc="316" width="14" height="8" />
		</element>

		<element ref="text_eme_j">
			<bounds xc="62.5" yc="316" width="14" height="8" />
		</element>

		<element ref="text_door_j">
			<bounds x="81" y="304" width="24" height="9" />
		</element>

		<!-- meters -->
		<element ref="text_throttle_j">
			<bounds x="120" y="306" width="27" height="10" />
		</element>
		<element name="counter0" ref="counter_throttle">
			<bounds x="150" y="306" width="10" height="10" />
		</element>

		<element ref="text_brake_j">
			<bounds x="179" y="306" width="27" height="10" />
		</element>
		<element name="counter1" ref="counter_brake">
			<bounds x="208" y="306" width="12" height="10" />
		</element>
	</view>

</mamelayout>
