	<!-- Orange program buttons -->
	<repeat count="16">
		<param name="prognum" start="1" increment="1"/>
		<element name="prog_button_~prognum~">
			<rect>
				<bounds x="0" y="0" width="20" height="21"/>
				<color state="0" red="0.99" green="0.38" blue="0.07"/>
				<color state="1" red="0.49" green="0.19" blue="0.04"/>
