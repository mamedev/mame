<mamelayout version="2">

	<element name="led_red" defstate="0">
		<disk state="1"><color red="1.0" green="0.1" blue="0.15" /></disk>
		<disk state="0"><color red="0.17" green="0.015" blue="0.02" /></disk>
	</element>

<!-- define elements -->
	<element name="static_black">
		<rect>
			<color red="0.0" green="0.0" blue="0.0" />
		</rect>
	</element>

	<element name="digit" defstate="0">
		<led7seg>
			<color red="1.0" green="0.3" blue="0.2" />
		</led7seg>
	</element>

	<view name="Internal Layout">
		<bounds left="0" right="110" top="0" bottom="40" />
		<element ref="static_black">
			<bounds left="0" right="110" top="0" bottom="40" />
		</element>

<!-- left panel -->
		<!-- Sonderspiele -->
		<element name="digit7" ref="digit">
			<bounds x="10" y="10" width="10" height="15" />
		</element>
		<element name="digit6" ref="digit">
			<bounds x="20" y="10" width="10" height="15" />
		</element>
		<element name="digit5" ref="digit">
			<bounds x="30" y="10" width="10" height="15" />
		</element>

<!-- right panel -->
		<!-- MÃ¼nzspeicher -->
		<element name="digit4" ref="digit">
			<bounds x="50" y="10" width="10" height="15" />
		</element>
		<element name="digit3" ref="digit">
			<bounds x="60" y="10" width="10" height="15" />
		</element>
		<element name="digit2" ref="digit">
			<bounds x="70" y="10" width="10" height="15" />
		</element>
		<element name="digit1" ref="digit">
			<bounds x="80" y="10" width="10" height="10" />
		</element>
		<element name="digit0" ref="digit">
			<bounds x="90" y="10" width="10" height="10" />
		</element>

<!-- control unit-->
		<element name="led0" ref="led_red">
			<bounds left="40" right="45" top="30" bottom="35" />
		</element>
		<element name="led1" ref="led_red">
			<bounds left="50" right="55" top="30" bottom="35" />
		</element>
	</view>
</mamelayout>
