		<text string="SYS" align="1" state="1">
			<color red="0.196" green="0.176" blue="0.235" />
		</text>
	</element>


<!-- build screen -->

	<view name="Internal Layout">
		<bounds left="0" top="0" right="125" bottom="30" />

		<screen index="0">
			<bounds x="15" y="-2" width="200" height="20" />
		</screen>

		<!-- 2nd line chars are about 0.83333 smaller -->
		<screen index="0">
			<bounds x="10" y="22" width="166.66" height="16.666" />
		</screen>

		<!-- draw background around lcd screen matrix part -->
		<element ref="static_bg">
			<bounds left="0" top="0" right="150" bottom="7" />
		</element>
		<element ref="static_bg">
			<bounds left="0" top="0" right="15.25" bottom="20" />
		</element>
		<element ref="static_bg">
			<bounds left="0" top="17.75" right="150" bottom="22.2" />
		</element>
		<element ref="static_bg">
			<bounds left="0" top="20" right="10.2" bottom="45" />
		</element>
		<element ref="static_bg">
			<bounds left="0" top="29.5" right="150" bottom="45" />
		</element>

		<!-- draw bezel around 2nd line -->
		<element ref="static_bg2">
			<bounds x="0" y="18.7" width="150" height="2.5" />
		</element>

		<element ref="static_bg2">
			<bounds x="0" y="20" width="7.9" height="15" />
		</element>
		<element ref="static_bg2">
			<bounds x="27.9" y="20" width="2.5" height="15" />
		</element>
		<element ref="static_bg2">
			<bounds x="50.4" y="20" width="2.5" height="15" />
		</element>
		<element ref="static_bg2">
			<bounds x="72.9" y="20" width="2.5" height="15" />
		</element>
		<element ref="static_bg2">
			<bounds x="95.4" y="20" width="2.5" height="15" />
		</element>
		<element ref="static_bg2">
			<bounds x="117.9" y="20" width="10" height="15" />
		</element>

		<!-- crop -->
		<element ref="static_black">
			<bounds left="0" top="-10" right="250" bottom="0" />
		</element>
		<element ref="static_black">
			<bounds left="125" top="-10" right="250" bottom="50" />
		</element>
		<element ref="static_black">
			<bounds left="0" top="30" right="250" bottom="50" />
		</element>

		<!-- draw lcd indicators -->

		<!-- left: _p, left -->
		<element name="seg70" ref="ind_p">
			<bounds x="1" y="9" width="5" height="5" />
		</element>
		<element name="seg71" ref="ind_left">
			<bounds x="7.5" y="8" width="4" height="6" />
		</element>

		<!-- right: run, sys -->
		<element name="seg3" ref="ind_run">
			<bounds x="115.5" y="6.5" width="8" height="5" />
		</element>
		<element name="seg4" ref="ind_sys">
			<bounds x="115.5" y="12" width="8" height="5" />
		</element>

		<!-- top: _low, _error, 2nd, inv, alpha, lc, ins -->
		<element name="seg40" ref="ind_low">
			<bounds x="1" y="1" width="12" height="5" />
		</element>
		<element name="seg43" ref="ind_error">
			<bounds x="16" y="1" width="15" height="5" />
		</element>

		<element name="seg41" ref="ind_2nd">
			<bounds x="32" y="1" width="7.5" height="5" />
		</element>
		<element name="seg44" ref="ind_inv">
			<bounds x="41" y="1" width="6.5" height="5" />
		</element>
		<element name="seg42" ref="ind_alpha">
			<bounds x="50" y="1" width="12" height="5" />
		</element>
		<element name="seg12" ref="ind_lc">
			<bounds x="64.3" y="1" width="4.5" height="5" />
		</element>
		<element name="seg11" ref="ind_ins">
			<bounds x="71.3" y="1" width="7" height="5" />
		</element>

		<!-- top: deg/rad/grad -->
		<element name="seg10" ref="ind_de">
			<bounds x="79.75" y="1" width="5.5" height="5" />
		</element>
		<element name="seg13" ref="ind_g">
			<bounds x="85" y="1" width="3" height="5" />
		</element>
		<element name="seg14" ref="ind_rad">
			<bounds x="88" y="1" width="7.75" height="5" />
		</element>

		<!-- top: hex, oct, i/o -->
		<element name="seg0" ref="ind_hex">
			<bounds x="97.7" y="1" width="7.5" height="5" />
		</element>
		<element name="seg1" ref="ind_oct">
			<bounds x="107" y="1" width="7.5" height="5" />
		</element>
		<element name="seg2" ref="ind_io">
			<bounds x="117.5" y="1" width="6" height="5" />
		</element>

	</view>
</mamelayout>
