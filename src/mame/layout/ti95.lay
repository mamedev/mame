	<element name="ind_run" defstate="0">
		<text string="RUN" align="1" state="0">
			<color red="0.515" green="0.537" blue="0.548" />
		</text>
		<text string="RUN" align="1" state="1">
			<color red="0.196" green="0.176" blue="0.235" />
