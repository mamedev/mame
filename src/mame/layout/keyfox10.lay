<mamelayout version="2">
	<!-- 7-segment LED display element -->
	<element name="digit" defstate="0">
		<led7seg><color red="1.0" green="0.2" blue="0.1" /></led7seg>
	</element>

	<!-- Decimal point -->
	<element name="dp" defstate="0">
		<disk state="1"><color red="1.0" green="0.2" blue="0.1" /></disk>
		<disk state="0"><color red="0.15" green="0.03" blue="0.015" /></disk>
	</element>

	<!-- Beat LED (green) -->
	<element name="beat_led" defstate="0">
		<disk state="1"><color red="0.2" green="1.0" blue="0.1" /></disk>
		<disk state="0"><color red="0.03" green="0.15" blue="0.015" /></disk>
	</element>

	<!-- Panel LED (red) -->
	<element name="panel_led" defstate="0">
		<disk state="1"><color red="1.0" green="0.1" blue="0.1" /></disk>
		<disk state="0"><color red="0.15" green="0.015" blue="0.015" /></disk>
	</element>

	<!-- Clickable button -->
	<element name="btn" defstate="0">
		<rect state="0"><color red="0.2" green="0.2" blue="0.25" /></rect>
		<rect state="1"><color red="0.4" green="0.6" blue="0.8" /></rect>
	</element>

