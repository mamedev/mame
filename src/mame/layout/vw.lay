<mamelayout version="2">

	<element name="led"> <disk state="0"> <color red="1.0" green="1.0" blue="0.1" /> </disk> </element>

	<element name="background">
		<rect>
			<bounds left="0" top="0" right="10" bottom="10" />
			<color red="0.2" green="0.2" blue="0.2" />
