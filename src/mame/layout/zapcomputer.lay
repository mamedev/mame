		</element>
		<element name="digit3" ref="digit">
			<bounds x="200" y="20" width="50" height="80" />
		</element>

		<element name="digit4" ref="digit">
			<bounds x="320" y="20" width="50" height="80" />
		</element>
		<element name="digit5" ref="digit">
