			<color red="1.0" green="0.3" blue="0.2" />
		</led7seg>
	</element>

    <element name="ladder" defstate="0">
		<rect state="1">
			<color red="1.0" green="1.0" blue="1.0" />
		</rect>
		<rect state="0">
			<color red="0.7" green="0.7" blue="0.7" />
		</rect>
	</element>

