		<disk state="1"><bounds x="0.1" y="0.1" width="0.9" height="0.9" /><color red="1.0" green="0.0" blue="0.0" /></disk>
		<disk state="0"><bounds x="0.1" y="0.1" width="0.9" height="0.9" /><color red="0.7" green="0.0" blue="0.0" /></disk>
		<text string="STOP"><bounds x="0.2" y="0.4" width="0.7" height="0.3" /><color red="0" green="0" blue="0" /></text>
