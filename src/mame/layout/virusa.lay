		<group ref="knob_base"><bounds x="0" y="10" width="75" height="70"/></group>

		<element ref="minus100pc_text"><bounds x="12" y="80" width="26" height="12"/></element>
		<element ref="0_text"><bounds x="36" y="0" width="6" height="12"/></element>
		<element ref="100pc_text"><bounds x="48" y="80" width="26" height="12"/></element>
	</group>

	<group name="knob_fall_rise">
		<element ref="gray_line"><bounds x="37" y="11" width="4" height="12"/></element>
		<group ref="knob_base"><bounds x="0" y="10" width="75" height="70"/></group>

		<element ref="fall_text"><bounds x="10" y="80" width="26" height="12"/></element>
		<element ref="infinity_text"><bounds x="30" y="-4" width="18" height="16"/></element>
		<element ref="rise_text"><bounds x="48" y="80" width="26" height="12"/></element>
	</group>

