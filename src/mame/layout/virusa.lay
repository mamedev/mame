	<element name="minus100pc_text"><text string="-100%"><color red="0.8" green="0.8" blue="0.8"/></text></element>
	<element name="infinity_text"><text string="âˆž"><color red="0.8" green="0.8" blue="0.8"/></text></element>
	<element name="fall_text"><text string="FALL"><color red="0.8" green="0.8" blue="0.8"/></text></element>
	<element name="rise_text"><text string="RISE"><color red="0.8" green="0.8" blue="0.8"/></text></element>

	<group name="knob_0_6_12">
		<group ref="knob_base"><bounds x="0" y="10" width="75" height="70"/></group>

		<element ref="0_text"><bounds x="15" y="80" width="6" height="12"/></element>
		<element ref="6_text"><bounds x="36" y="0" width="6" height="12"/></element>
		<element ref="12_text"><bounds x="52" y="80" width="13" height="12"/></element>
	</group>

	<group name="knob_1plus2">
		<element ref="gray_line"><bounds x="37" y="11" width="4" height="12"/></element>
		<group ref="knob_base"><bounds x="0" y="10" width="75" height="70"/></group>

		<element ref="1_text"><bounds x="15" y="80" width="6" height="12"/></element>
		<element ref="1plus2_text"><bounds x="30" y="0" width="17" height="12"/></element>
		<element ref="2_text"><bounds x="52" y="80" width="6" height="12"/></element>
	</group>

	<group name="knob_6">
		<element ref="gray_line"><bounds x="37" y="11" width="4" height="12"/></element>
		<group ref="knob_base"><bounds x="0" y="10" width="75" height="70"/></group>

		<element ref="minus6_text"><bounds x="13" y="80" width="13" height="12"/></element>
		<element ref="0_text"><bounds x="36" y="0" width="6" height="12"/></element>
		<element ref="6_text"><bounds x="52" y="80" width="13" height="12"/></element>
	</group>

	<group name="knob_saturation">
		<!-- FIXME: This one should have dots only in the left half
		    and the word "SATURATION" along a circular path to the right.
		    but I don't know how to draw that in the .lay schema
		-->
		<element ref="gray_line"><bounds x="37" y="11" width="4" height="12"/></element>
		<group ref="knob_base"><bounds x="0" y="10" width="75" height="70"/></group>

		<element ref="minus6_text"><bounds x="15" y="80" width="19" height="12"/></element>
		<element ref="0_text"><bounds x="36" y="0" width="6" height="12"/></element>
		<element ref="6_text"><bounds x="52" y="80" width="13" height="12"/></element>
	</group>

