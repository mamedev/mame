	<group name="sb_board">
		<bounds x="0" y="0" width="130" height="130" />

		<element ref="cwhite"><bounds x="0" y="0" width="130" height="130" /></element>

		<!-- recesses (distracting, so make it optional) -->
		<collection name="Board Recesses" visible="no">
			<repeat count="13">
				<param name="y" start="0" increment="10" />
				<repeat count="13">
					<param name="x" start="0" increment="10" />
					<element ref="recess"><bounds x="~x~" y="~y~" width="10" height="10" /></element>
				</repeat>
			</repeat>
		</collection>

		<element ref="board_bg"><bounds x="0" y="0" width="130" height="130" /></element>

		<!-- sensors, pieces -->
		<repeat count="13">
			<param name="y" start="0" increment="10" />
			<param name="i" start="13" increment="-1" />

			<element ref="hlbb" inputtag="board:RANK.~i~" inputmask="0x01">  <bounds x="0"   y="~y~" width="10" height="10" /><color alpha="0.04" /></element>
