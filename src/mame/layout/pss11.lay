	<element name="start_stop"><text string="START/STOP" /></element>
	<element name="tempo"><text string="&#x25bc;TEMPO&#x25b2;" /></element>
	<element name="volume"><text string="&#x25bc;VOLUME&#x25b2;" /></element>
	<element name="mode"><text string="MUSIC MODE SELECTOR" align="1" /></element>
	<element name="style"><text string="STYLE" align="2" /></element>
	<element name="voice"><text string="VOICE" align="2" /></element>
	<element name="song"><text string="SONG" align="2" /></element>
