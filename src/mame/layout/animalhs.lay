			<bounds x="2.72" y="3.10" width="0.46" height="0.20" />
		</element>


		<element name="lamp13" ref="start_b" inputtag="IN0" inputmask="0x80">
			<bounds x="3.45" y="3.08" width="0.50" height="0.24" />
		</element>
		<element name="lamp13" ref="start" inputtag="IN0" inputmask="0x80">
			<bounds x="3.47" y="3.10" width="0.46" height="0.20" />
