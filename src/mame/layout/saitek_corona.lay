		<rect state="1"><color red="1" green="0" blue="0" /></rect>
	</element>
	<element name="ledg" defstate="0">
		<rect state="0"><color red="0" green="0" blue="0" /></rect>
		<rect state="1"><color red="0" green="1" blue="0" /></rect>
	</element>

	<element name="led" defstate="0">
