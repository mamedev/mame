<mamelayout version="2">

	<element name="nothing"><rect><color alpha="0" /></rect></element>

	<element name="overlay" defstate="0">
		<rect state="0">
			<bounds left="0" top="0" right="1" bottom="1" />
			<color red="1.0" green="0.894" blue="0.341" />
		</rect>
		<rect state="1">
			<bounds left="0" top="0" right="1" bottom="1" />
			<color red="0" green="0.586" blue="1.0" />
		</rect>
	</element>

	<view name="Color Overlay" showpointers="no">
		<screen index="0">
			<bounds left="0" top="0" right="4" bottom="3" />
		</screen>
		<element ref="overlay" blend="multiply" inputtag="DSW2" inputmask="0x80">
			<bounds left="0" top="0" right="4" bottom="3" />
		</element>
		<element ref="nothing" clickthrough="no"> <!-- block clickable input -->
			<bounds left="0" top="0" right="4" bottom="3" />
		</element>
	</view>

</mamelayout>
