		</text>
	</element>
	<element name="HOLD">
		<text string="HOLD">
			<color red="1.0" green="1.0" blue="1.0" />
		</text>
	</element>
	<element name="INTE">
		<text string="INTE">
			<color red="1.0" green="1.0" blue="1.0" />
		</text>
	</element>
	<element name="background">
		<rect>
			<bounds left="0" top="0" right="1" bottom="1" />
			<color red="0.0" green="0.0" blue="0.0" />
		</rect>
	</element>

