<!-- Basic display of LEDs for the MMD-2 -->

<mamelayout version="2">

	<element name="r_led">
		<disk>
			<color red="1.0" green="0.0" blue="0.0" />
		</disk>
	</element>

	<element name="g_led">
		<disk>
			<color red="0.0" green="1.0" blue="0.0" />
		</disk>
	</element>

	<element name="o_led">
		<disk>
			<color red="1.0" green="0.50" blue="0.0" />
		</disk>
	</element>

	<element name="r_digit" defstate="0">
		<led7seg>
			<color red="1.0" green="0.0" blue="0.0" />
		</led7seg>
	</element>

	<element name="PORT0">
		<text string="PORT 0">
			<color red="1.0" green="1.0" blue="1.0" />
		</text>
	</element>
	<element name="PORT1">
		<text string="PORT 1">
