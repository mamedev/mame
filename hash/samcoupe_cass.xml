			<dataarea name="cass" size="67044">
				<rom name="snare - alternate - side b.tzx" size="67044" crc="3278d671" sha1="9046c7b2ca54c8dd653cc797f7cc2ac6616791af"/>
			</dataarea>
		</part>
	</software>
